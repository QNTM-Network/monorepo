{
  "manifest_version": 2,
  "name": "Quantmn",
  "version": "0.1",
  "icons": {
    "16": "../src/public/logo.png",
    "48": "../src/public/logo.png",
    "128": "../src/public/logo.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.ts"]
  },
  "content_scripts": [{
          "matches": [ "http://*/*", "https://*/*"],
    "js": ["content-script.ts"]
  }],
  "permissions":
    ["tabs", "storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "web_accessible_resources": ["../src/public/*"]

}

