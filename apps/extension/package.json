{
  "name": "extension",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "parcel watch src/manifest.json --host localhost --config @parcel/config-webextension --target dev",
    "build": "parcel build src/manifest.json --config @parcel/config-webextension --target dev",
    "bundle": "run-s && parcel src/manifest.json src/index.html --target dev",
    "cypress": "cypress open"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "cypress": "10.1.0",
    "events": "^3.3.0",
    "parcel": "^2.7.0",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@headlessui/react": "^1.7.2",
    "@maticnetwork/maticjs": "^3.5.0",
    "@maticnetwork/maticjs-ethers": "^1.0.2",
    "@metamask/detect-provider": "^1.2.0",
    "@self.id/framework": "^0.3.3",
    "@types/lodash": "^4.14.185",
    "alchemy-sdk": "^2.1.0",
    "axios": "^0.27.2",
    "babel-runtime": "^6.26.0",
    "crx-hotreload": "^1.0.6",
    "date-fns": "^2.29.3",
    "ethers": "^5.7.0",
    "flatted": "^3.2.7",
    "key-did-provider-ed25519": "^2.0.1",
    "lodash": "^4.17.21",
    "metamask-extension-provider": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "sass": "^1.54.9",
    "scss": "^0.2.4",
    "streamr-client": "^6.0.9"
  },
  "targets": {
    "dev": {
      "distDir": "dist",
      "optimize": false,
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    }
  }
}
